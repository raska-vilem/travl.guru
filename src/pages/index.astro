---

import { readSingleton } from '@directus/sdk';
import DefaultLayout from '@/layouts/default.astro';
import { directusRequest } from '@/lib/directus';
import Navbar from '@/organisms/navbar.astro';

const global = await directusRequest(readSingleton('global'));
---

<DefaultLayout title={global.title} description={global.description}>
    <Navbar/>
</DefaultLayout>
